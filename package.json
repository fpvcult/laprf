{
  "name": "@fpvcult/laprf",
  "description": "A Node.js LapRF Protocol Library",
  "repository": "github:fpvcult/laprf",
  "version": "0.4.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/laprf.js",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "rollup -c && tsc --declaration --emitDeclarationOnly --declarationDir dist",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:nofix": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet",
    "prepare": "npm run lint:nofix && npm run test && npm run build",
    "test": "jest"
  },
  "keywords": [
    "fpv",
    "drone",
    "race"
  ],
  "author": "John Hooks <bitmachina@outlook.com>",
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.1.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "rollup": "^2.38.5",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}
