{
  "name": "@fpvcult/laprf",
  "description": "A Node.js LapRF Protocol Library",
  "repository": "github:fpvcult/laprf",
  "version": "0.3.0",
  "main": "dist/laprf.js",
  "typings": "dist/laprf.d.ts",
  "files": [
    "dist/laprf.js",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "rollup -c && shx cp src/types.d.ts dist/laprf.d.ts",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:nofix": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet",
    "prepare": "npm run lint:nofix && npm run test && npm run build",
    "test": "jest"
  },
  "keywords": [
    "fpv",
    "drone",
    "race"
  ],
  "author": "John Hooks <bitmachina@outlook.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@bitmachina/binary": "^0.3.0",
    "debug": "^4.3.1",
    "supports-color": "^7.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^3.1.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "rollup": "^1.32.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.4.4",
    "tslib": "^1.14.1",
    "typescript": "^4.0.5"
  },
  "browserslist": "node 12.0"
}
